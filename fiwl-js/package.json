{
    "name": "fiwl-js",
    "displayName": "fiwl-js",
    "description": "Syntax highlighter for FIWL js",
    "version": "0.0.1",
    "publisher": "fiwl-js",
    "repository": "https://github.com/fiwl-js/vscode-syntax",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "FIWL",
                "aliases": [
                    "FIWL",
                    "FIWL"
                ],
                "extensions": [
                    ".fiwl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "FIWL",
                "scopeName": "main.fiwl",
                "path": "./syntaxes/FIWL.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.14.0"
    }
}
